@Article{2020_Polcz_SCL,
  author   = {P{\'{e}}ter Polcz and Tam{\'{a}}s P{\'{e}}ni and Bal{\'{a}}zs Kulcs{\'{a}}r and G{\'{a}}bor Szederk{\'{e}}nyi},
  title    = {Induced {L}2-gain computation for rational {LPV} systems using {F}insler{\textquoteright}s lemma and minimal generators},
  journal  = {Systems \& Control Letters},
  year     = {2020},
  volume   = {142},
  pages    = {104738},
  issn     = {0167-6911},
  abstract = {This paper proposes a novel method to compute an upper bound on the induced L2-gain for a linear parameter varying (LPV) system with rational parameter dependence. The proposed method relies on a standard dissipation inequality condition. The storage function is a quadratic function of the state and a rational function of the parameters. The specific parameter dependence is restricted to involve (fixed) rational functions and an affine term with free decision variables. Finsler’s lemma and affine annihilators are used to formulate sufficient linear matrix inequality (LMI) conditions for the dissipativity relation. The dimension and conservatism of the resulting LMI problem are reduced by the joint application of minimal generators and maximal annihilators. An LPV model of a pendulum–cart system is used to demonstrate the proposed method and compare it to existing techniques in the literature.},
  comment  = {[1_SAJAT_ujsag_SCI]},
  doi      = {10.1016/j.sysconle.2020.104738},
  file     = {2019_Polcz_SCL_submission.pdf:Sajat/2019_Polcz_SCL_submission.pdf:PDF;2019_Polcz_SCL_submitted.pdf:Sajat/2019_Polcz_SCL_submitted.pdf:PDF;2020_Polcz_SCL.pdf:Sajat/2020_Polcz_SCL.pdf:PDF},
  groups   = {30_ECC_ipend},
  keywords = {POLCZ,SCI,JRN,UNKP,Linear parameter varying systems, Stability, Induced -gain, Linear fractional transformation, Rational Lyapunov function},
}

