@Article{2015_Kuang.etal,
  author          = {Kuang, Y. and Zhang, L. and Yi, Z.},
  title           = {Constructing l1-graphs for subspace learning via recurrent neural networks},
  journal         = {Pattern Analysis and Applications},
  year            = {2015},
  volume          = {18},
  number          = {4},
  pages           = {817--828},
  note            = {cited By 0},
  __markedentry   = {[ppolcz:]},
  abstract        = {In this paper, the problem that we are interested is constructing $$l_{1}$$l1-graphs for subspace learning via recurrent neural networks. We propose a closed affine subspace learning (CASL) method to do so. The problem of CASL is formulated as an optimization problem described by an energy function in the non-negative orthant. The sufficient conditions for keeping the minimum points of the energy function in the non-negative orthant are given. A model of Lotka–Volterra recurrent neural networks is constructed to solve the optimization problem in terms of these sufficient conditions. It shows that the set of stable attractors of the network model just equals the set of minimum points of the energy function in the non-negative orthant. Based on these equivalences, the problem of CASL can be solved by running the proposed LV RNNs to obtain the stable attractors. The $$l_{1}$$l1-graphs then can be constructed. Experiments on some synthetic and real data show that the $$l_{1}$$l1-graphs constructed in this way are more effective, especially when processing the data sampled from multiple manifolds. © 2014, Springer-Verlag London.},
  author_keywords = {Closed affine subspace learning (CASL); Lotka–Volterra recurrent neural networks (LV RNNs); Multiple manifolds clustering and embedding; Non-negative matrix factorization (NMF); Sparse representation},
  document_type   = {Article},
  doi             = {10.1007/s10044-014-0370-1},
  groups          = {Embedding},
  publisher       = {Springer London},
  source          = {Scopus},
}

