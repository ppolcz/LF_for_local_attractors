@InProceedings{2020_Girgis.etal,
  author    = {A. M. {Girgis} and J. {Park} and C. -F. {Liu} and M. {Bennis}},
  title     = {Predictive control and communication co-design: {A} {G}aussian process regression approach},
  booktitle = {2020 IEEE 21\textsuperscript{st} International Workshop on Signal Processing Advances in Wireless Communications (SPAWC)},
  year      = {2020},
  pages     = {1--5},
  month     = may,
  abstract  = {While remote control over wireless connections is a key enabler for scalable control systems consisting of multiple actuator-sensor pairs, i.e., control systems, it entails two technical challenges. Due to the lack of wireless resources, only a limited number of control systems can be served, making the state observations outdated. Further, even after scheduling, the state observations received through wireless channels are distorted, hampering control stability. To address these issues, in this article we propose a scheduling algorithm that reduces the age-of-information (AoI) of the last received states. Meanwhile, for non-scheduled sensor-actuator pairs, we propose a machine learning (ML) aided predictive control algorithm, in which states are predicted using a Gaussian process regression (GPR). Since the GPR prediction credibility decreases with the AoI of the input data, both predictive control and AoI-based scheduler should be co-designed. Hence, we formulate a joint scheduling and transmission power optimization via the Lyapunov optimization framework. Numerical simulations corroborate that the proposed co-designed predictive control and AoI based scheduling achieves lower control errors, compared to a benchmark scheme using a round-robin scheduler without state prediction.},
  doi       = {10.1109/SPAWC48557.2020.9154304},
  file      = {2020_Girgis.etal.pdf:2020_Girgis.etal.pdf:PDF},
  groups    = {MPC, Gaussian_Process, Learning},
  issn      = {1948-3252},
  keywords  = {actuators;control system synthesis;feedback;Gaussian processes;learning (artificial intelligence);Lyapunov methods;optimisation;predictive control;regression analysis;scheduling;sensors;stability;wireless channels;state observations;wireless channels;control stability;scheduling algorithm;received states;nonscheduled sensor-actuator pairs;GPR prediction credibility;AoI-based scheduler;joint scheduling;machine learning aided predictive control algorithm;Lyapunov optimization;numerical simulation;predictive control and communication co-design;wireless resources;multiple actuator-sensor pairs;scalable control systems;wireless connections;remote control;Gaussian process regression approach;state prediction;Control systems;Wireless communication;Ground penetrating radar;Wireless sensor networks;Covariance matrices;Signal to noise ratio;Gaussian processes;6G;predictive control;age of information;communication and control co-design;Gaussian process regression},
}

