@InProceedings{2016_Bobiti.Lazar,
  author    = {Bobiti, Ruxandra and Lazar, Mircea},
  title     = {A sampling approach to finding {Lyapunov} functions for nonlinear discrete-time systems},
  booktitle = {European Control Conference (ECC)},
  year      = {2016},
  pages     = {561--566},
  month     = jun,
  abstract  = {This paper considers the problem of stability verification for discrete-time nonlinear systems via {Lyapunov} functions. Depending on the system dynamics, the candidate {Lyapunov} function and the set of initial states of interest, one generally needs to handle large, possibly non-convex or non-feasible optimization problems. To avoid such problems, we propose a constructive and systematically applicable sampling-based approach to stability analysis of nonlinear systems. This approach proposes verification of the decrease condition for a candidate {Lyapunov} function on a finite sampling of a bounded set of initial conditions and then it extends the validity of the {Lyapunov} function to an infinite set of initial conditions by exploiting continuity properties. This result involves no apriori analytic description of the continuity property and it is based on multi-resolution sampling, to perform efficient state-space exploration. Moreover, the stability verification is decentralized in the sampling points, which makes the method parallelizable. The proposed methodology is illustrated on two examples.},
  comment   = {Biography Ruxandra Bobiti was born in Tr. Severin, Romania, on 10\textsuperscript{th} of July 1988. In 2011, she received her degree in Systems Engineering from the Faculty of Automatic Control and Computer Science at the ?Politehnica? University of Bucharest, Romania, on Model Predictive Control of a 3D crane system. In 2011 she started the courses of the Systems and Control Master Program of TU/e with the TU/e Amandus H. Lundqvist Scholarship Program. The topic of her research for the master thesis was on robustness and stability of discrete-time systems via finite-time {Lyapunov} functions, with an application to Power Systems, with the supervision of Dr. Mircea Lazar. The Master of Science degree was received with Cum Laudae. https://www.tue.nl/en/university/departments/electrical-engineering/department/staff/detail/ep/e/d/ep-uid/20116901/},
  doi       = {10.1109/ECC.2016.7810344},
  file      = {2016_Bobiti.Lazar_A_sampling_approach_to_finding_LF_for_nonlinear_DT_systems.pdf:2016_Bobiti.Lazar.pdf:PDF},
  groups    = {DT, Stability, Simulation_guided},
  keywords  = {Lyapunov methods;discrete time systems;nonlinear control systems;sampling methods;stability;state-space methods;Lyapunov functions;multiresolution sampling;nonlinear discrete-time systems;sampling approach;stability analysis;stability verification;state-space exploration;Asymptotic stability;Lyapunov methods;Nonlinear dynamical systems;Optimization;Stability analysis;System dynamics, rank4},
