@Article{2020_Paulson.Mesbah,
  author   = {J. A. {Paulson} and A. {Mesbah}},
  title    = {Approximate closed-loop robust model predictive control with guaranteed stability and constraint satisfaction},
  journal  = {IEEE Control Systems Letters},
  year     = {2020},
  volume   = {4},
  number   = {3},
  pages    = {719--724},
  month    = jul,
  issn     = {2475-1456},
  abstract = {The real-time implementation of closed-loop robust model predictive control (MPC) schemes is an important challenge for fast systems, as their solution complexity depends strongly on the system size, control policy parametrization, and prediction horizon. We look to address this problem by approximating the implicitly-defined MPC controller using deep learning. Although the resulting neural network approximation has a small memory footprint and can be efficiently computed, it does not guarantee robust constraint satisfaction or stability. We propose a novel projection-based strategy that is capable of providing a certificate of robust feasibility and input-to-state stability in real-time. We also show how this projection operator can be formulated as a parametric quadratic program that is solvable offline. The advantages of the proposed approach are demonstrated on a benchmark case study.},
  doi      = {10.1109/LCSYS.2020.2980479},
  file     = {2020_Paulson.Mesbah.pdf:2020_Paulson.Mesbah.pdf:PDF},
  groups   = {MPC},
  keywords = {approximation theory;closed loop systems;learning systems;neurocontrollers;predictive control;quadratic programming;robust control;parametric quadratic program;projection operator;small memory footprint;deep learning;solution complexity;MPC schemes;approximate closed-loop robust model predictive control;input-to-state stability;robust constraint satisfaction;neural network approximation;implicitly-defined MPC controller;prediction horizon;control policy parametrization;system size;guaranteed stability;Robustness;Stability analysis;Numerical stability;Real-time systems;Neural networks;Optimization;Asymptotic stability;Robust model predictive control;deep neural networks;input-to-state stability;safe invariant sets},
}

@Article{2020_Peaucelle.Leduc,
