@InProceedings{2018_Seilerb,
  author        = {P. {Seiler}},
  title         = {An iterative algorithm to estimate invariant sets for uncertain systems},
  booktitle     = {2018 Annual American Control Conference (ACC)},
  year          = {2018},
  pages         = {4249--4254},
  month         = jun,
  __markedentry = {[ppolcz:4]},
  abstract      = {This paper develops an iterative algorithm to estimate an invariant set for uncertain systems. The uncertain system is given as a connection of a nominal linear time-invariant system and a perturbation. The input/output behavior of the perturbation is described by integral quadratic constraints (IQCs). The proposed approach incorporates IQCs into a dissipation inequality formulation. One issue is that it is often useful to specify the IQC in the frequency domain or, equivalently, in the time-domain as a “soft” infinite-horizon constraint. However, the dissipation inequality formulation requires constraints that are valid over all finite time horizons. The main technical result is a finite-horizon bound on soft IQCs constructed using a state-feedback transformation. This forms the basis for the proposed iterative algorithm to estimate invariant sets. A simple example is provided to demonstrate the proposed approach.},
  doi           = {10.23919/ACC.2018.8431113},
  file          = {2018_Seilerb.pdf:2018_Seilerb.pdf:PDF},
  groups        = {IQC},
  issn          = {2378-5861},
  keywords      = {frequency-domain analysis;iterative methods;linear systems;state feedback;uncertain systems;iterative algorithm;uncertain system;nominal linear time-invariant system;integral quadratic constraints;IQC;time-domain;soft infinite-horizon constraint;invariant sets estimation;perturbation;state-feedback transformation;frequency domain;Time-domain analysis;Frequency-domain analysis;Games;Iterative methods;Uncertain systems;Perturbation methods;Linear systems},
}

@InProceedings{2018_Seiler,
  author        = {P. {Seiler}},
  title         = {{IQC} analysis of uncertain {LTV} systems with rational dependence on time},
  booktitle     = {2018 IEEE Conference on Decision and Control (CDC)},
  year          = {2018},
  pages         = {7213--7218},
  month         = dec,
  __markedentry = {[ppolcz:4]},
  abstract      = {The goal of this paper is to assess the robustness of an uncertain linear time-varying (LTV) system on a finite time horizon. The uncertain system is modeled as a connection of a known LTV system and a perturbation. The state matrices of the LTV system are assumed to be rational functions of time. This is used to model the uncertain LTV system as an connection of a time invariant system and an augmented perturbation that includes time. The input/output behavior of the perturbation is described by time-domain, integral quadratic constraints (IQCs). Static and dynamic IQCs are developed for the multiplication by time. A sufficient condition to bound the induced L2 gain is formulated using dissipation inequalities and IQCs. The approach is demonstrated with two simple examples.},
