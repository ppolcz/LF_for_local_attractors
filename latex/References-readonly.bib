@Article{2016_Mesbah,
  author        = {Ali Mesbah},
  title         = {Stochastic model predictive control: {A}n overview and perspectives for future research},
  journal       = {IEEE Control Systems Magazine},
  year          = {2016},
  volume        = {36},
  number        = {6},
  pages         = {30--44},
