@InProceedings{2016_MinZhou.etal,
  author    = {{Min Zhou} and {Zhao-Qin Guo} and {Xiang Li}},
  title     = {Design of model predictive control for time-varying nonlinear system based on gaussian process regression modeling},
  booktitle = {2016 IEEE 21\textsuperscript{st} International Conference on Emerging Technologies and Factory Automation (ETFA)},
  year      = {2016},
  pages     = {1--6},
  month     = sep,
  abstract  = {In this work, modeling and control of a time-varying nonlinear system is studied. The modeling is under the Gaussian process regression (GPR) framework, which is a data-driven approach. To address he time-varying phenomenon which commonly exists in the manufacturing processes, an online GPR algorithm is adopted. Next, model predictive control (MPC) is designed based on the obtained GPR model which aims to minimize the difference between the actual process output and the reference or target. The advantage of incorporating the GPR modeling in the MPC design is that, GPR based prediction not only gives the predicted output but also the associated prediction uncertainty. By utilizing the additional information, the tolerance for prediction uncertainty is set as one of the constrains in the MPC design. From practical point of view, the GPR based MPC design provides the opportunity for safe operation or quality control by knowing and setting the tolerance for the prediction uncertainty, which is significantly important in manufacturing process control. The effectiveness of the proposed methods are validated through simulation based studies.},
  doi       = {10.1109/ETFA.2016.7733737},
  file      = {2016_MinZhou.etal.pdf:2016_MinZhou.etal.pdf:PDF},
  groups    = {MPC, Gaussian_Process, Learning},
  keywords  = {Gaussian processes;manufacturing processes;nonlinear control systems;predictive control;process control;regression analysis;time-varying systems;model predictive control;time-varying nonlinear system;Gaussian process regression modeling;online GPR algorithm;MPC;data-driven approach;safe operation;quality control;manufacturing process control;Ground penetrating radar;Mathematical model;Predictive models;Gaussian processes;Uncertainty;Predictive control;Modeling;Model Predictive control;Gaussian process models;Time-varying systems;Online Modeling},
}

