@InProceedings{2010_Seiler.etal,
  author    = {P. {Seiler} and A. {Packard} and G. J. {Balas}},
  title     = {A dissipation inequality formulation for stability analysis with integral quadratic constraints},
  booktitle = {49\textsuperscript{th} IEEE Conference on Decision and Control (CDC)},
  year      = {2010},
  pages     = {2304--2309},
  month     = dec,
  abstract  = {Integral quadratic constraints (IQCs) provide a general framework for robustness analysis of feedback inter-connections. The main IQC stability theorem by Megretski and Rantzer was formulated with frequency domain conditions that depend on the IQC multiplier. Their proof of this theorem uses a homotopy method and operator theory. An interesting aspect of this theory is that input/output stability (defined as uniformly bounded gain over all finite horizons) is established using integral constraints that only hold, in general, on infinite time horizons. The use of IQCs that only hold over infinite time horizons is related to the use of noncausal multipliers in absolute stability theory. This paper shows that if the conditions of the IQC stability theorem are satisfied by any rational IQC multiplier then a dissipation inequality is satisfied by a quadratic storage function. This provides a new interpretation for IQC analysis in terms of quadratic storage functions and a causal, finite-horizon dissipation inequality.},
  doi       = {10.1109/CDC.2010.5717073},
  file      = {2010_Seiler.etal.pdf:2010_Seiler.etal.pdf:PDF},
  groups    = {ISS, IQC},
  issn      = {0191-2216},
  keywords  = {absolute stability;constraint theory;feedback;frequency-domain analysis;infinite horizon;input-output stability;integral equations;interconnected systems;dissipation inequality formulation;integral quadratic constraint;robustness analysis;feedback interconnection;IQC stability theorem;frequency domain condition;homotopy method;operator theory;input-output stability;infinite time horizon;noncausal multiplier;absolute stability theory;rational IQC multiplier;quadratic storage function;finite-horizon dissipation inequality;Stability analysis;Time domain analysis;Frequency domain analysis;Linear matrix inequalities;Polynomials;Linear systems},
}

