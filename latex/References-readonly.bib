@Article{2019_Carron.etal,
  author   = {A. {Carron} and E. {Arcari} and M. {Wermelinger} and L. {Hewing} and M. {Hutter} and M. N. {Zeilinger}},
  title    = {Data-driven model predictive control for trajectory tracking with a robotic arm},
