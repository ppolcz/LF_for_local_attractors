@Article{2013_Mathew.etal,
  author   = {Navin John Mathew and K. Koteswara Rao and N. Sivakumaran},
  title    = {Swing up and stabilization control of a rotary inverted pendulum},
  journal  = {IFAC Proceedings Volumes},
  year     = {2013},
