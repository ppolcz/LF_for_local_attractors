@Article{2015_Soehret.etal,
  author   = {Yasin {{\c{S}}}{\"{o}}hret and Ali Din{\c{c}} and T. Hikmet Karako{\c{c}}},
  title    = {Exergy analysis of a turbofan engine for an unmanned aerial vehicle during a surveillance mission},
  journal  = {Energy},
