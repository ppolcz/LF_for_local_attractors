  keywords  = {approximation theory;Gaussian processes;mobile robots;nonlinear control systems;predictive control;road vehicles;vehicle dynamics;Gaussian process dynamics;autonomous miniature race cars;adaptive high performance control method;racing dynamics;cautious nonlinear model predictive control approach;dynamics model;residual model uncertainty;dynamically adjusting inducing inputs;real-time implementable controller;model learning;NMPC;racing performance;Automobiles;Data models;Gaussian processes;Uncertainty;Tires;Computational modeling;Predictive models},
}

@InProceedings{2018_Hewing.Zeilinger,
  author    = {L. {Hewing} and M. N. {Zeilinger}},
  title     = {Stochastic model predictive control for linear systems using probabilistic reachable sets},
