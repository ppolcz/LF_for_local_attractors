}

@Article{2018_Polcz_EJC2,
  author   = {Polcz, P{\'{e}}ter and P{\'{e}}ni, Tam{\'{a}}s and Szederk{\'{e}}nyi, G{\'{a}}bor},
  title    = {Computational method for estimating the domain of attraction of discrete-time uncertain rational systems},
  journal  = {European Journal of Control},
  year     = {2019},
  volume   = {49},
  pages    = {68--83},
  issn     = {0947-3580},
  abstract = {Using linear matrix inequality (LMI) conditions, we propose a computational method to generate {Lyapunov} functions and to estimate the domain of attraction (DOA) of uncertain nonlinear (rational) discrete-time systems. The presented method is a discrete-time extension of the approach first presented in ([18]), where the authors used Finsler{\textquoteright}s lemma and affine annihilators to give sufficient LMI conditions for stability. The system representation required for DOA computation is generated systematically by using the linear fractional transformation (LFT). Then a model simplification step not affecting the computed {Lyapunov} function (LF) is executed on the obtained linear fractional representation (LFR). The LF is computed in a general quadratic form of a state and parameter dependent vector of rational functions, which are generated from the obtained LFR model. The proposed method is compared to the numeric n-dimensional order reduction technique proposed in ([35]). Finally, additional tuning knobs are proposed to obtain more degrees of freedom in the LMI conditions. The method is illustrated on two benchmark examples.},
  comment  = {[1_SAJAT_ujsag_SCI] EJC2 2018},
  doi      = {10.1016/j.ejcon.2018.12.004},
  file     = {2018_Polcz_EJC2_olvashato.pdf:Sajat/2018_Polcz_EJC2_olvashato.pdf:PDF;2018_Polcz_EJC2_published.pdf:Sajat/2018_Polcz_EJC2_published.pdf:PDF},
  groups   = {30_ECC_ipend},
  keywords = {POLCZ,SCI,JRN,KAP,OTKA,Nonlinear system, {Lyapunov} function, Domain of attraction, Linear matrix inequality, Model simplification, rank4},
}

