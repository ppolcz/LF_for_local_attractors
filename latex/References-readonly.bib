@Article{2018_Gritli.Belghith,
  author   = {Hass{\`{e}}ne Gritli and Safya Belghith},
  title    = {Robust feedback control of the underactuated inertia wheel inverted pendulum under parametric uncertainties and subject to external disturbances: {LMI} formulation},
